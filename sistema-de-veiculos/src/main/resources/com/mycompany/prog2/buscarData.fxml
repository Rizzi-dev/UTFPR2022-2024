<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="650.0" minHeight="650.0" prefHeight="650.0" spacing="20.0" style="-fx-background-color: #444444; -fx-border-color: ffffff; -fx-border-width: 5;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.prog2.BuscarDataController">
    <children>
      <Label ellipsisString="0" style="-fx-text-fill: EDEDED;" text="BUSCAR VEICULO/DATA" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <TableView fx:id="tabela" prefHeight="347.0" prefWidth="550.0">
        <columns>
            <TableColumn fx:id="colunaID" prefWidth="41.0" text="ID" />
            <TableColumn fx:id="colunaDevolucao" prefWidth="117.0" text="Data Devolução" />
            <TableColumn fx:id="colunaRetirada" prefWidth="125.0" text="Data Retirada" />
          <TableColumn fx:id="colunaMotorista" prefWidth="123.0" text="Motorista" />
          <TableColumn fx:id="colunaVeiculo" prefWidth="143.0" text="Veículo" />
        </columns>
      </TableView>
      <VBox alignment="CENTER" prefHeight="51.0" prefWidth="550.0" spacing="10.0">
         <children>
            <Button onMouseClicked="#cancelar" prefHeight="25.0" prefWidth="564.0" style="-fx-background-color: #171717; -fx-text-fill: EDEDED;" text="Voltar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
